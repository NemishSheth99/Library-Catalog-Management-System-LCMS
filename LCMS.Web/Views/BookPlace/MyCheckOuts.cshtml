@model IEnumerable<LCMS.Models.BookPlace.BookPlaceDetail>

@{
    ViewBag.Title = "MyCheckOuts";
    Layout = "~/Views/Shared/_PageLayout.cshtml";
}


@if (Model != null && Model.Count() != 0)
{
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PlaceCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaterialType)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.BorrowedOn)
        </th>
        <th></th>
    </tr>

   
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.PlaceCode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MaterialType)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BorrowedOn)
                </td>
                <td>
                    <a href="/BookPlace/CheckInBook/@Html.DisplayFor(modelItem => item.Id)" class="btn-sm btn-primary">Chech In</a>
                </td>
            </tr>
        }
   
</table>
}
else
{
    <h6>No Data Found!!!</h6>
}
